    <string name="route_description">The path of a particular climb, or a predefined set of moves.</string>
    <string name="crag">Crag</string>
    <string name="crag_description">A small area with climbing routes, often just a small cliff face or a few boulders.</string>
    <string name="artificial">Artificial Climbing Area</string>
    <string name="artificial_description">Artificial climbing walls including gyms and other man made structures.</string>
    <string name="unknown">Unknown</string>
    <string name="unknown_description">Node marked with the climbing tag. No other relevat tags.</string>

